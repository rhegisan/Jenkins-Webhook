    agent any

    stages {
        stage('Build') {
            steps {
                echo 'Builded'
            }
        }
        stage('Test') {
            steps {
                echo 'Tested'
            }
        }
        stage('Deploy') {
            steps {
                echo 'Deployed'
            }
        }
        stage('Release') {
            steps {
                echo 'Released'
            }
        }
    }
}
